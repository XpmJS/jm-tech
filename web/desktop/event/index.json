{
      "cname": "活动首页",
      "data": {
            "page":{
                  "title":"活动"
            },
            "user": {
                  "api": "/xpmsns/user/user/getUserInfo",
                  "query": {}
            },
            "categories": {
                  "api": "/xpmsns/pages/category/search",
                  "query": {
                        "select": "category_id,name,highlight,link,isblank,slug",
                        "order": "priority asc",
                        "noParentId": "1",
                        "children": false,
                        "isnav": 1
                  } 
            },
            "services": {
                  "api": "/xpmsns/pages/category/search",
                  "query": {
                        "select": "category_id,name,highlight,link,isblank,slug",
                        "order": "priority asc",
                        "noParentId": "1",
                        "children": false,
                        "isfootnav": 1
                  }
            },
            "navs": "{{categories.data}}",
            "curr": {
                "api": "/xpmsns/pages/category/get",
                "query": {
                    "slug": "event",
                    "select": "name,slug,link,isfootnav,isnav"
                }
            },
            "active":{
                "nav": "{{curr.slug}}"
            },
            "site": {
                  "api": "/xpmsns/pages/siteconf/get",
                  "query": {
                        "site_slug": "global",
                        "select": "*"
                  }
            },
            "events": {
                  "api": "/xpmsns/pages/event/search",
                  "query": {
                        "page": "1",
                        "perpage": "12",
                        "select": "event_id,title,slug,link,deadline,cover,hosts,organizers,begin,end,quota,user_cnt,series.name,series.slug,series.series_id",
                        "series": "{{__get.series}}",
                        "order": "begin"
                  }
            },
            "__css": [
                  "/js/trix/trix.css"
            ],
            "__js": [
                  "/js/trix/trix.js",
                  "/js/jquery-validation-1.17.0/jquery.validate.min.js",
                  "/js/jquery.ui.widget.js",
                  "/js/jquery.iframe-transport.js",
                  "/js/jquery.fileupload.js",
                  "/js/jquery.form.min.js",
                  "/js/mustache.min.js"
            ]
      },
      "entries": [{
                  "method": "GET",
                  "router": "/event/index",
                  "ttl": 0
            },
            {
                  "method": "GET",
                  "router": "/event/index.html",
                  "ttl": 0
            }
      ],
      "alias": {
            "mobile": "/mobile/event/index",
            "wechat": "/mobile/event/index"
      }
}