{
      "cname": "活动首页",
      "data": {
            "page":{
                  "title":"活动"
            },
            "user": {
                  "api": "/xpmsns/user/user/getUserInfo",
                  "query": {}
            },
            "categories": {
                  "api": "/xpmsns/pages/category/search",
                  "query": {
                        "select": "category_id,name,highlight,link,isblank,slug",
                        "order": "priority asc",
                        "noParentId": "1",
                        "children": false,
                        "isnav": 1
                  } 
            },
            "services": {
                  "api": "/xpmsns/pages/category/search",
                  "query": {
                        "select": "category_id,name,highlight,link,isblank,slug",
                        "order": "priority asc",
                        "noParentId": "1",
                        "children": false,
                        "isfootnav": 1
                  }
            },
            "navs": "{{categories.data}}",
            "nav": "null",
            "site": {
                  "api": "/xpmsns/pages/siteconf/get",
                  "query": {
                        "site_slug": "global",
                        "select": "*"
                  }
            },
            "questions": {
                  "api": "/xpmsns/qanda/question/search",
                  "query": {
                        "page": "1",
                        "perpage": "12",
                        "select": "question_id,question.title,question.summary,question.content,user.name,category.name,tags,user.user_id",
                        "publish_desc": "1"
                  }
            },
            "recommend": {
                  "api": "/xpmsns/pages/recommend/getContents",
                  "query": {
                        "page": "1",
                        "perpage": "12",
                        "slugs": "qanda_recommend"
                  }
            },
            "hot_tags":{
                  "api": "/xpmsns/pages/tag/search",
                  "query": {
                        "page": "1",
                        "perpage": "8",
                        "question_desc": "1"
                  }
            },
            "__css": [
                  "/js/trix/trix.css"
            ],
            "__js": [
                  "/js/trix/trix.js",
                  "/js/jquery-validation-1.17.0/jquery.validate.min.js",
                  "/js/jquery.ui.widget.js",
                  "/js/jquery.iframe-transport.js",
                  "/js/jquery.fileupload.js",
                  "/js/jquery.form.min.js",
                  "/js/mustache.min.js"
            ]
      },
      "entries": [{
                  "method": "GET",
                  "router": "/event/index",
                  "ttl": 0
            },
            {
                  "method": "GET",
                  "router": "/event/index.html",
                  "ttl": 0
            }
      ],
      "alias": {
            "mobile": "/mobile/event/index",
            "wechat": "/mobile/event/index"
      }
}