{
      "cname": "活动首页",
      "data": {
            "page":{
                  "title":"活动"
            },
            "user": {
                  "api": "/xpmsns/user/user/getUserInfo",
                  "query": {}
            },
            "categories": {
                  "api": "/xpmsns/pages/category/search",
                  "query": {
                        "select": "category_id,name,highlight,link,isblank,slug",
                        "order": "priority asc",
                        "noParentId": "1",
                        "children": false,
                        "isnav": 1
                  } 
            },
            "services": {
                  "api": "/xpmsns/pages/category/search",
                  "query": {
                        "select": "category_id,name,highlight,link,isblank,slug",
                        "order": "priority asc",
                        "noParentId": "1",
                        "children": false,
                        "isfootnav": 1
                  }
            },
            "navs": "{{categories.data}}",
            "active":{
                "nav": "event"
            },
            "site": {
                  "api": "/xpmsns/pages/siteconf/get",
                  "query": {
                        "site_slug": "global",
                        "select": "*"
                  }
            },
            "__css": [],
            "__js": []
      },
      "entries": [
            {"method":"GET", "router":"/event/detail/{id:[0-9a-zA-Z_]+}.html", "ttl":0},
	        {"method":"GET", "router":"/event/detail/{id:[0-9a-zA-Z_]+}", "ttl":0}
      ],
      "alias": {
            "mobile": "/mobile/event/index",
            "wechat": "/mobile/event/index"
      }
}