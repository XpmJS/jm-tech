{
      "cname": "问答",
      "data": {
            "page":{
                  "title":"{{__var.tag}}-标签-知否"
            },
            "tag":"{{__var.tag}}",
            "user": {
                  "api": "/xpmsns/user/user/getUserInfo",
                  "query": {}
            },
            "categories": {
                  "api": "/xpmsns/pages/category/search",
                  "query": {
                        "select": "category_id,name,highlight,link,isblank,slug",
                        "order": "priority asc",
                        "noParentId": "1",
                        "children": false,
                        "isnav": 1
                  } 
            },
            "services": {
                  "api": "/xpmsns/pages/category/search",
                  "query": {
                        "select": "category_id,name,highlight,link,isblank,slug",
                        "order": "priority asc",
                        "noParentId": "1",
                        "children": false,
                        "isfootnav": 1
                  }
            },
            "navs": "{{categories.data}}",
            "nav": "null",
            "curr": {
                "api": "/xpmsns/pages/category/get",
                "query": {
                    "slug": "qanda",
                    "select": "name,slug,link,isfootnav,isnav"
                }
            },
            "active":{
                "nav": "{{curr.slug}}"
            },
            "site": {
                  "api": "/xpmsns/pages/siteconf/get",
                  "query": {
                        "site_slug": "global",
                        "select": "*"
                  }
            },
            "questions": {
                  "api": "/xpmsns/qanda/question/search",
                  "query": {
                        "page": "1",
                        "perpage": "12",
                        "tags":"{{__var.tag}}",
                        "select": "question_id,question.title,user.name,category.name,tags,user.user_id",
                        "publish_desc": "1"
                  }
            }
      },
      "entries": [{
                  "method": "GET",
                  "router": "/qanda/tag/{tag}",
                  "ttl": 3600
            },
            {
                  "method": "GET",
                  "router": "/qanda/tag/{tag}",
                  "ttl": 0
            },
            {
                  "method": "GET",
                  "router": "/qanda/tag/{tag}.html",
                  "ttl": 0
            }
      ],
      "alias": {
            "mobile": "/mobile/qanda/tag",
            "wechat": "/mobile/qanda/tag"
      }
}