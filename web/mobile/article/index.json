{	
	"cname":"首页-手机版",
	"data": {
		"categories":{
			"api":"/xpmsns/pages/category/search",
			"query":{
				"select":"category_id,name,highlight,link,isblank,slug",
				"order":"priority asc",
				"children":false,
				"noParentId":"1",
				"isnav":1
			}
		},
		"wechat":{
			"api":"/xpmsns/pages/article/wechat",
			"query":{}
		},
		"site":{
			"api":"/xpmsns/pages/siteconf/get",
			"query":{
				"site_slug":"global",
				"select":"*"
			}
		},
		"navs":"{{categories.data}}",
		"r": {
			"api":"/xpmsns/pages/recommend/getContents",
			"query":{
				"slugs":"focus,latest,section_1"
			}
		},
		"section":"{{r.section_1}}",
		"sections":{
			"api":"/xpmsns/pages/recommend/search",
			"query":{
				"select":"title,slug",
				"pos":"index"
			}
		},
		"links":{
			"api":"/xpmsns/pages/links/search",
			"query":{
				"select":"name,link"
			}
		},
		"seo": {
			"title":"{{site.site_name}}",
			"keywords":"{{site.site_name}}",
			"description":"{{site.site_intro}}"
		},
		"page":{
            "title":"首页"
        },
        "tabBar":{
            "color":"#555555",
            "selectedColor": "#888888",
            "backgroundColor": "#ffffff",
            "borderStyle": "black",
            "position":"bottom",
            "list":[
                { "text":"资讯", "pagePath":"/m/home/index", "iconPath":"{{__STOR__::/__PROJECT_NAME/mobile/assets}}/images/icon_footer_my.svg", "selectedIconPath":"{{__STOR__::/__PROJECT_NAME/mobile/assets}}/images/icon_footer_my_active.svg", "selected":true},
                { "text":"问答", "pagePath":"/m/home/index", "iconPath":"{{__STOR__::/__PROJECT_NAME/mobile/assets}}/images/icon_footer_my.svg", "selectedIconPath":"{{__STOR__::/__PROJECT_NAME/mobile/assets}}/images/icon_footer_my_active.svg"},
                { "text":"视频", "pagePath":"/m/home/index", "iconPath":"{{__STOR__::/__PROJECT_NAME/mobile/assets}}/images/icon_footer_my.svg", "selectedIconPath":"{{__STOR__::/__PROJECT_NAME/mobile/assets}}/images/icon_footer_my_active.svg"},
                { "text":"图集", "pagePath":"/m/home/index", "iconPath":"{{__STOR__::/__PROJECT_NAME/mobile/assets}}/images/icon_footer_my.svg", "selectedIconPath":"{{__STOR__::/__PROJECT_NAME/mobile/assets}}/images/icon_footer_my_active.svg"},
                { "text":"我的", "pagePath":"/m/home/index", "iconPath":"{{__STOR__::/__PROJECT_NAME/mobile/assets}}/images/icon_footer_my.svg", "selectedIconPath":"{{__STOR__::/__PROJECT_NAME/mobile/assets}}/images/icon_footer_my_active.svg"}
            ],
            "custom":false
        }
	},
	"entries":[
		{"method":"GET", "router":"/m", "ttl":0},
		{"method":"GET", "router":"/m/article", "ttl":0}
	],
	
	"alias": {
		"desktop":"/desktop/article/index",
		"wxapp":"/article/index"
	}
}