{	
	"cname":"首页-手机版",
	"data": {
		"categories":{
			"api":"/xpmsns/pages/category/search",
			"query":{
				"select":"category_id,name,highlight,link,slug",
				"order":"priority asc",
				"children":false,
				"noParentId":"1",
				"isnav":1
			}
		},
		"wechat":{
			"api":"/xpmsns/pages/article/wechat",
			"query":{}
		},
		"site":{
			"api":"/xpmsns/pages/siteconf/get",
			"query":{
				"site_slug":"global",
				"select":"*"
			}
		},
		"navs":"{{categories.data}}",
		"r": {
			"api":"/xpmsns/pages/recommend/getContents",
			"query":{
				"slugs":"focus,latest,section_1"
			}
		},
		"section":"{{r.section_1}}",
		"sections":{
			"api":"/xpmsns/pages/recommend/search",
			"query":{
				"select":"title,slug",
				"pos":"index"
			}
		},
		"links":{
			"api":"/xpmsns/pages/links/search",
			"query":{
				"select":"name,link"
			}
		},
		"seo": {
			"title":"{{site.site_name}}",
			"keywords":"{{site.site_name}}",
			"description":"{{site.site_intro}}"
		},
		"page":{
			"title":"首页"
		}
	},
	"entries":[
		{"method":"GET", "router":"/m", "ttl":0},
		{"method":"GET", "router":"/m/article", "ttl":0}
	],
	
	"alias": {
		"desktop":"/desktop/article/index",
		"wxapp":"/article/index"
	}
}