{	
	"cname":"图集详情页(三级)-手机版",
	"data": {
		"categories":{
			"api":"/xpmsns/pages/category/search",
			"query":{
				"select":"category_id,name,highlight,link,isblank,slug",
				"order":"priority asc",
				"noParentId":"1",
				"children":false,
				"isnav":1
			}
		},
		"services":{
			"api":"/xpmsns/pages/category/search",
			"query":{
				"select":"category_id,name,highlight,link,isblank,slug",
				"order":"priority asc",
				"noParentId":"1",
				"children":false,
				"isfootnav":1
			}
		},
		"navs":"{{categories.data}}",
		"album":{
			"api":"/xpmsns/pages/album/get",
			"query":{
				"album_id":"{{__var.id}}"
			}
		},
		"relation":{
			"api":"/xpmsns/pages/recommend/getContents",
			"query":{
				"perpage":18,
				"slug":"video_relation",
				"keywords":"{{article.keywords}}",
				"exclude_articles":"{{article.article_id}}"
			}
		},
		"site":{
			"api":"/xpmsns/pages/siteconf/get",
			"query":{
				"site_slug":"global",
				"select":"*"
			}
		},
		"links":{
			"api":"/xpmsns/pages/links/search",
			"query":{
				"select":"name,link"
			}
		},
		"page":{
            "title":"返回图片",
            "back":"/m/album/index",
			"logo":"{{site.site_logo}}"
		},
		"seo": {
			"title":"{{album.title}}",
			"keywords":"{{site.seo_keywords}}",
			"description":"{{site.seo_summary}}"
		},
		"active": {
			"nav":"nothing"
		}
	},
	"entries":[
		{"method":"GET", "router":"/m/album/{id:[0-9a-zA-Z_]+}.html", "ttl":0},
		{"method":"GET", "router":"/m/album/{id:[0-9a-zA-Z_]+}", "ttl":0}
	],
	
	"alias": {
		"desktop":"/desktop/album/detail",
		"wxapp": "/album"
	}
}