{	
	"cname":"页面部件-推荐列表-图集(手机端)",
	"type":"component",
	"data": {
		"site":{
			"api":"/xpmsns/pages/siteconf/get",
			"query":{
				"site_slug":"global",
				"select":"*"
			}
		},
		"section":{
			"api":"/xpmsns/pages/recommend/getContents",
			"query":{
				"slug":"{{__var.slug}}",
				"page":"{{__get.page}}",
				"perpage":18
			}
		}
	},
	"entries":[
		{"method":"GET", "router":"/m/widget/recommend/album/{slug:[0-9a-zA-Z_]+}", "ttl":0}
	],
	"alias": {
		"desktop":"/desktop/widget/recommend/album",
		"wxapp": "/widget/recommend/album"
	}
}